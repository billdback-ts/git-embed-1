guid: c23774ae-cf0c-4b21-a6e7-da313274c0c1
table:
  name: FACT_RETAPP_SALES
  db: RETAILAPPAREL
  schema: PUBLIC
  db_table: FACT_RETAPP_SALES
  connection:
    name: Retail Sales Data
    fqn: ec07b957-d37f-4af6-8cd8-4c48e051c0b9
  columns:
  - name: Sales ID
    db_column_name: SALESID
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
