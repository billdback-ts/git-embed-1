      index_type: DONT_INDEX
      currency_type:
        iso_code: USD
    db_column_properties:
      data_type: DOUBLE
  - name: Sales Amount
    db_column_name: SALES
    properties:
      column_type: MEASURE
      aggregation: SUM
      index_type: DONT_INDEX
      currency_type:
        iso_code: USD
    db_column_properties:
      data_type: DOUBLE
  - name: Sales Date
    db_column_name: RECORDDATE
    properties:
      column_type: ATTRIBUTE
      index_type: DONT_INDEX
    db_column_properties:
